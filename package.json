{
  "name": "quizzer",
  "version": "0.4.54",
  "private": true,
  "main": "public/electron.js",
  "author": "SKP-000",
  "description": "App for designing and participating in quizzes",
  "homepage": "./",
  "build": {
    "appId": "com.electron.quizzer",
    "productName": "Quizzer",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public",
      "*.js"
    ],
    "directories": {
      "buildResources": "assets",
      "app": "./",
      "output": "./dist/"
    },
    "win": {
      "target": "nsis"
    },
    "buildVersion": "0.4.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.6",
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@types/axios": "^0.14.0",
    "@types/he": "^1.1.0",
    "@types/history": "^4.7.3",
    "@types/jest": "24.0.18",
    "@types/js-base64": "^2.3.1",
    "@types/node": "12.7.4",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.5",
    "@types/styled-components": "^4.1.19",
    "@types/uuid": "^3.4.5",
    "axios": "^0.19.0",
    "clsx": "^1.0.4",
    "connected-react-router": "^6.5.2",
    "electron-is-dev": "^1.1.0",
    "final-form": "^4.18.5",
    "final-form-arrays": "^3.0.1",
    "he": "^1.2.0",
    "js-base64": "^2.5.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.1",
    "react-keyboard-event-handler": "^1.5.2",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "react-sortable-hoc": "^1.10.1",
    "react-spring": "^8.0.27",
    "redux": "^4.0.4",
    "redux-observable": "^1.1.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.3",
    "styled-components": "^4.3.2",
    "typesafe-actions": "^4.4.2",
    "typescript": "3.6.2",
    "uifx": "^2.0.3",
    "utility-types": "^3.7.0",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder build --win -c.extraMetadata.main=build/electron.js --publish never",
    "preelectron-pack": "react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "devtron": "^1.4.0",
    "electron": "^9.4.0",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-react-devtools": "^0.5.3",
    "react-redux-typescript-scripts": "^1.6.2",
    "redux-devtools-extension": "^2.13.8",
    "tslint": "^5.20.0",
    "tslint-react": "^4.0.0",
    "wait-on": "^3.3.0"
  }
}
